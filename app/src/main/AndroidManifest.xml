<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="by.dvd.pull_up">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:configChanges="orientation|screenSize">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AllResultsActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".DateListActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".ExerciseResultsActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".DescriptionExerciseActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".DescriptionExerciseListActivity" />
        <activity
            android:name=".EditDateActivity"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".GraphActivity" />
    </application>

</manifest>